# ~/.zshrc

# export
export EDITOR='vim'
export LANG='ja_JP.UTF-8'

# init sheldon
eval "$(sheldon source)"

# init starship
eval "$(starship init zsh)"

# rust path
source "$HOME/.cargo/env"

# zoxide
eval "$(zoxide init zsh)"

# alias
alias ls=eza
alias cat=bat
alias fd-find=find
alias grep=rg
alias tree=broot
alias top=zenith


